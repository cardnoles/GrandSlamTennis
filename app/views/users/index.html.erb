

<% @users.each do |user| %>
  <div>
    id: <%= user[:id] %> <br>
    first name: <%= user[:first_name] %> <br>
    last name: <%= user[:last_name] %><br>
    user name: <%= user[:user_name] %><br>
    email: <%= user[:email] %><br>
    password: <%= user[:password] %><br>
    admin: <%= user[:admin] %><br>

    </div>
    <br>
<% end %>
