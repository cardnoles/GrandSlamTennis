<% provide(:title, "edit country info") %>
<body id="admins-page">
<p><%=link_to "Back", '/admins' %></p>

<h3>Edit a country's information</h3>

<%= form_for @country do |f| %>
  <p>
    <%= f.label :country %><br>
    <%= f.text_field :country %>
  </p>

   <p>
    <%= f.label :country_code %><br>
    <%= f.text_field :country_code %>
  </p>

  <p>
    <%= f.label :fed_name %><br>
    <%= f.text_field :fed_name %>
  </p>

  <p>
    <%= f.label :fed_address %><br>
    <%= f.text_field :fed_address %>
  </p>

  <p>
    <%= f.label :fed_city %><br>
    <%= f.text_field :fed_city %>
  </p>

  <p>
    <%= f.label :telephone %><br>
    <%= f.text_field :telephone %>
  </p>

  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :president %><br>
    <%= f.text_field :president %>
  </p>

  <p>
    <%= f.label :website %><br>
    <%= f.text_field :website %>
  </p>

    <p>
    <%= f.label :flag %><br>
    <%= f.text_field :flag %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
</body>

